<h1><%= @recipes_type_default.name%></h1>
<% if @recipes_type_default.recipes.empty?%>
  Nenhuma receita encontrada para este tipo de receitas
<%end%>
<%= render 'home/left_up' %>
<% @recipes_type_default.recipes.each do |recipe|%>
  <div class="col-md-6">
    <%= render recipe %>
  </div>
<%end%>
<%= render 'home/left_down' %>

<div class="row">
  <div class="col-md-12"><%= link_to('Voltar', root_path)%></div>
</div>
