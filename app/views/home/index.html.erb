<h1>CookBook</h1>
<hr>
  <%= link_to("Enviar uma receita", new_recipe_path)%>
<hr>
<p>Bem-vindo ao maior livro de receitas online</p>
<% @recipes.each do |recipe| %>
  <%= render recipe %>
<%end%>

<% @cuisines.each do |cuisine| %>
  <li><%= link_to("#{cuisine.name}", cuisine) %>
<% end %>

<% @recipes_type.each do |recipe_type| %>
  <li><%= link_to("#{recipe_type.name}", recipe_type) %>
<% end %>
